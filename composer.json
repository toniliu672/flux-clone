{
  "name": "flux-clone/ui",
  "description": "A beautiful Blade UI component library for Laravel & Livewire - Flux UI alternative",
  "keywords": [
    "laravel",
    "livewire",
    "blade",
    "components",
    "ui",
    "tailwind"
  ],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Toni"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0|^12.0",
    "livewire/livewire": "^3.0",
    "blade-ui-kit/blade-heroicons": "^2.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0|^10.0",
    "pestphp/pest": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "FluxClone\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FluxClone\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "FluxClone\\FluxCloneServiceProvider"
      ],
      "aliases": {
        "FluxClone": "FluxClone\\Facades\\FluxClone"
      }
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
